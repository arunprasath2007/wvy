<div class="message me{{transparent_emoji text}}" data-message="{{id}}">

    <div class="bubble">
        <div class="text">{{emojione text}}</div>
    </div>

    <div class="status status-sent" title="Sent">
        <svg class="i i-18"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#check-circle-outline"></use></svg>
    </div>
</div>
