@model Weavy.Core.Models.File
<tr data-href="@Model.Url()">
    <td class="table-icon">@Html.SvgIcon(Model.IconName(), color: Model.IconColor())</td>
    <td><a href="@Model.Url()">@Model.Title</a></td>
    <td>@Html.Time(Model.ModifiedAt, format: "g")</td>
    <td>@Model.Kind</td>
    <td>@FileHelper.FileSizeAsString(Model.Size)</td>
    <td class="table-icon">@{ Html.RenderPartial("_FileMenu", Model);}</td>
</tr>
